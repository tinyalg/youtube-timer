{
  "manifest_version": 3,
  "name": "YouTube Timer",
  "version": "0.2",
  "description": "YouTube等の視聴時間を計測します。",
  "author": "Tinyalg Systems",
  "homepage_url": "https://tinyalg.github.io/youtube-timer/",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "YouTube Time",
    "default_popup": "popup.html"  
  }
}